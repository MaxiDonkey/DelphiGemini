program GeminiGoogle;

uses
  Vcl.Forms,
  GeminiTest in 'GeminiTest.pas' {Form1},
  Vcl.Themes,
  Vcl.Styles,
  Gemini.Interactions.StreamEngine in 'source\Gemini.Interactions.StreamEngine.pas',
  Gemini.Interactions.StreamCallbacks in 'source\Gemini.Interactions.StreamCallbacks.pas';

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  TStyleManager.TrySetStyle('Windows11 MineShaft');
  Application.CreateForm(TForm1, Form1);
  Application.Run;
end.
